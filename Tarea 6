        "\r\n",
        "N = int(input(\"Colocar el numero N\\n\"))\r\n",
        "\r\n",
        "Band = True\r\n",
        "I = 1\r\n",
        "\r\n",
        "while ( I <= N ):\r\n",
        "  if (Band == True):\r\n",
        "    Serie = Serie + 1/I\r\n",
        "    Band = False\r\n",
        "  else:\r\n",
        "    Serie = Serie - 1/I\r\n",
        "    Band = True\r\n",
        "  I = I + 1\r\n",
        "\r\n",
        "print(Serie)"
